{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport http from \"../../http-common\";\nexport default {\n  name: \"AllQuizzes\",\n  data() {\n    return {\n      quizzes: [] // Массив для хранения всех викторин\n    };\n  },\n  methods: {\n    // Получение всех викторин\n    fetchAllQuizzes() {\n      http.get(\"/quizList\").then(response => {\n        // Добавляем свойство showDescription для каждой викторины\n        this.quizzes = response.data.map(quiz => ({\n          ...quiz,\n          showDescription: false // Изначально описание скрыто\n        }));\n      }).catch(error => {\n        console.log(\"Ошибка при получении викторин: \", error);\n      });\n    },\n    // Метод для начала викторины\n    startQuiz(quizId) {\n      this.$router.push({\n        path: `/quiz/${quizId}`\n      }); // Здесь предполагается, что у вас есть маршрут для прохождения викторины\n    },\n    // Метод для переключения видимости описания\n    toggleDescription(quizId) {\n      const quiz = this.quizzes.find(q => q.id === quizId);\n      if (quiz) {\n        quiz.showDescription = !quiz.showDescription; // Переключаем видимость описания\n      }\n    }\n  },\n  mounted() {\n    this.fetchAllQuizzes(); // Получаем все викторины при монтировании компонента\n  }\n};","map":{"version":3,"names":["http","name","data","quizzes","methods","fetchAllQuizzes","get","then","response","map","quiz","showDescription","catch","error","console","log","startQuiz","quizId","$router","push","path","toggleDescription","find","q","id","mounted"],"sources":["C:\\Users\\Samedi\\Documents\\SecondProject\\SecondProject\\frontend\\src\\components\\quiz\\QuizList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\">\r\n    <h3 class=\"text-center mb-4\">Все викторины</h3>\r\n\r\n    <div v-if=\"quizzes.length\" class=\"row justify-content-center\">\r\n      <div class=\"col-md-8\">\r\n        <ul class=\"list-group\">\r\n          <li v-for=\"quiz in quizzes\" :key=\"quiz.id\" class=\"list-group-item\">\r\n            <div class=\"d-flex justify-content-between align-items-center\">\r\n              <span>{{ quiz.name }}</span>\r\n              <div>\r\n                <button class=\"btn btn-sm btn-info mr-2\" @click=\"toggleDescription(quiz.id)\">\r\n                  {{ quiz.showDescription ? \"Скрыть описание\" : \"Показать описание\" }}\r\n                </button>\r\n                <button class=\"btn btn-sm btn-primary\" @click=\"startQuiz(quiz.id)\">Пройти викторину</button>\r\n              </div>\r\n            </div>\r\n            <!-- Описание викторины -->\r\n            <div v-if=\"quiz.showDescription\" class=\"mt-2 p-2\" style=\"background-color: #e7f3fe; border-radius: 5px;\">\r\n              <p>{{ quiz.description }}</p>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else class=\"text-center\">\r\n      <p>Викторин пока нет.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from \"../../http-common\";\r\n\r\nexport default {\r\n  name: \"AllQuizzes\",\r\n  data() {\r\n    return {\r\n      quizzes: [] // Массив для хранения всех викторин\r\n    };\r\n  },\r\n  methods: {\r\n    // Получение всех викторин\r\n    fetchAllQuizzes() {\r\n      http\r\n        .get(\"/quizList\")\r\n        .then(response => {\r\n          // Добавляем свойство showDescription для каждой викторины\r\n          this.quizzes = response.data.map(quiz => ({\r\n            ...quiz,\r\n            showDescription: false // Изначально описание скрыто\r\n          }));\r\n        })\r\n        .catch(error => {\r\n          console.log(\"Ошибка при получении викторин: \", error);\r\n        });\r\n    },\r\n\r\n    // Метод для начала викторины\r\n    startQuiz(quizId) {\r\n      this.$router.push({ path: `/quiz/${quizId}` }); // Здесь предполагается, что у вас есть маршрут для прохождения викторины\r\n    },\r\n\r\n    // Метод для переключения видимости описания\r\n    toggleDescription(quizId) {\r\n      const quiz = this.quizzes.find(q => q.id === quizId);\r\n      if (quiz) {\r\n        quiz.showDescription = !quiz.showDescription; // Переключаем видимость описания\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchAllQuizzes(); // Получаем все викторины при монтировании компонента\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Стили для кнопок и списка викторин */\r\n.list-group-item {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AAiCA,OAAOA,IAAG,MAAO,mBAAmB;AAEpC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAC,CAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,eAAeA,CAAA,EAAG;MAChBL,IAAG,CACAM,GAAG,CAAC,WAAW,EACfC,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAI,CAACL,OAAM,GAAIK,QAAQ,CAACN,IAAI,CAACO,GAAG,CAACC,IAAG,KAAM;UACxC,GAAGA,IAAI;UACPC,eAAe,EAAE,KAAI,CAAE;QACzB,CAAC,CAAC,CAAC;MACL,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAAC,iCAAiC,EAAEF,KAAK,CAAC;MACvD,CAAC,CAAC;IACN,CAAC;IAED;IACAG,SAASA,CAACC,MAAM,EAAE;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAG,SAAQH,MAAO;MAAE,CAAC,CAAC,EAAE;IAClD,CAAC;IAED;IACAI,iBAAiBA,CAACJ,MAAM,EAAE;MACxB,MAAMP,IAAG,GAAI,IAAI,CAACP,OAAO,CAACmB,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,EAAC,KAAMP,MAAM,CAAC;MACpD,IAAIP,IAAI,EAAE;QACRA,IAAI,CAACC,eAAc,GAAI,CAACD,IAAI,CAACC,eAAe,EAAE;MAChD;IACF;EACF,CAAC;EACDc,OAAOA,CAAA,EAAG;IACR,IAAI,CAACpB,eAAe,CAAC,CAAC,EAAE;EAC1B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}