 формы\r\n                const data = {\r\n                    text: this.question.text,\r\n                    answer: this.question.answer,\r\n                    quizId: this.question.quizId\r\n                };\r\n\r\n                // Отправка данных на сервер\r\n                http\r\n                    .post(\"/api/addQuestion\", data)\r\n                    .then(response => {\r\n                        // Очистка формы для добавления следующего вопроса\r\n                        this.question.text = \"\";\r\n                        this.question.answer = \"\";\r\n                    })\r\n                    .catch((e) => {\r\n                        console.log(e);\r\n                    });\r\n            }\r\n        }\r\n    };\r\n</script>\r\n"],"mappings":"AA+CI,OAAOA,IAAG,MAAO,mBAAmB;AACpC,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,MAAK,CAAE;MACtC;IACJ,CAAC;EACL,CAAC;EACDG,OAAO,EAAE;IACLC,WAAWA,CAACC,CAAC,EAAE;MACXA,CAAC,CAACC,cAAc,CAAC,CAAC,EAAE;MACpB,MAAMV,IAAG,GAAI;QACTE,IAAI,EAAE,IAAI,CAACD,QAAQ,CAACC,IAAI;QACxBC,MAAM,EAAE,IAAI,CAACF,QAAQ,CAACE,MAAM;QAC5BC,MAAM,EAAE,IAAI,CAACH,QAAQ,CAACG;MAC1B,CAAC;;MAED;MACAN,IAAG,CACEa,IAAI,CAAC,kBAAkB,EAAEX,IAAI,EAC7BY,IAAI,CAACC,QAAO,IAAK;QACd;QACA,IAAI,CAACZ,QAAQ,CAACC,IAAG,GAAI,EAAE;QACvB,IAAI,CAACD,QAAQ,CAACE,MAAK,GAAI,EAAE;MAC7B,CAAC,EACAW,KAAK,CAAEL,CAAC,IAAK;QACVM,OAAO,CAACC,GAAG,CAACP,CAAC,CAAC;MAClB,CAAC,CAAC;IACV;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}