\";\r\n\r\nexport default {\r\n  name: \"AllQuizzes\",\r\n  data() {\r\n    return {\r\n      quizzes: [] // Массив для хранения всех викторин\r\n    };\r\n  },\r\n  methods: {\r\n    // Получение всех викторин\r\n    fetchAllQuizzes() {\r\n      http\r\n        .get(\"/quizList\")\r\n        .then(response => {\r\n          this.quizzes = response.data; // Записываем полученные викторины в массив\r\n        })\r\n        .catch(error => {\r\n          console.log(\"Ошибка при получении викторин: \", error);\r\n        });\r\n    },\r\n\r\n    // Метод для начала викторины\r\n    startQuiz(quizId) {\r\n      this.$router.push({ path: `/quiz/${quizId}` }); // Здесь предполагается, что у вас есть маршрут для прохождения викторины\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchAllQuizzes(); // Получаем все викторины при монтировании компонента\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Стили для кнопок и списка викторин */\r\n.list-group-item {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AAsBA,OAAOA,IAAG,MAAO,mBAAmB;AAEpC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAC,CAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,eAAeA,CAAA,EAAG;MAChBL,IAAG,CACAM,GAAG,CAAC,WAAW,EACfC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACL,OAAM,GAAIK,QAAQ,CAACN,IAAI,EAAE;MAChC,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAAC,iCAAiC,EAAEF,KAAK,CAAC;MACvD,CAAC,CAAC;IACN,CAAC;IAED;IACAG,SAASA,CAACC,MAAM,EAAE;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAG,SAAQH,MAAO;MAAE,CAAC,CAAC,EAAE;IAClD;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACb,eAAe,CAAC,CAAC,EAAE;EAC1B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}